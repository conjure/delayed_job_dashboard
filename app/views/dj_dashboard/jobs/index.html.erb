<h1>Jobs</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>Queue</th>
      <th>Priority</th>
      <th>Attempts</th>
      <th>Method</th>
      <th>Created At</th>
      <th>Run At</th>
      <th>Failed At</th>
    </tr>
  </thead>

  <tbody>
    <%= render partial: 'job', collection: @jobs %>
  </tbody>
</table>
